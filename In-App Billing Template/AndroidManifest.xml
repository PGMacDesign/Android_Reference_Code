<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pgmacdesign.inappbillingtest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    
  	<uses-permission android:name="com.android.vending.BILLING" />    

  	
	  	
	<application
	    android:allowBackup="true"
	    android:icon="@drawable/ic_launcher"
	    android:label="@string/app_name"
	    android:theme="@style/AppTheme" >
	    
	    
	    <activity
	      android:name="com.pgmacdesign.inappbillingtest.StartUpActivity"
	      android:label="@string/activity_startup_label"
	      android:screenOrientation="portrait" >
	      <intent-filter>
	        <action android:name="android.intent.action.MAIN" />
	
	        <category android:name="android.intent.category.LAUNCHER" />
	      </intent-filter>
	    </activity>
	    <activity
	      android:name="com.pgmacdesign.inappbillingtest.MainActivity"
	      android:label="@string/activity_main_label"
	      android:screenOrientation="portrait" />
	    <activity
	      android:name="com.pgmacdesign.inappbillingtest.PurchasePassportActivity"
	      android:label="@string/activity_purchase_label"
	      android:screenOrientation="portrait" />
	</application>
  		

    
    
</manifest>
